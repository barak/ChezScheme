diff --git a/csug/Makefile b/csug/Makefile
index a078abc..952ec14 100644
--- a/csug/Makefile
+++ b/csug/Makefile
@@ -80,7 +80,7 @@ $(x).presecondrun: $(x).firstrun
 	cat tspl.idx >> $x.idx
