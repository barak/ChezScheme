#!/bin/sh

set -e

if [ "$1" = "configure" ]; then
    update-alternatives \
        --install /usr/bin/scheme-script scheme-script /usr/bin/chezscheme9.5 10 \
        --slave /usr/share/man/man1/scheme-script.1.gz scheme-script.1.gz \
                /usr/share/man/man1/scheme-script9.5.1.gz
fi

